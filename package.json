{
    "name": "@ethossoftworks/outcome",
    "description": "An alternative approach to async/await error handling for TypeScript",
    "authors": "Ryan Mitchener",
    "version": "1.1.0",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/ethossoftworks/outcome-ts.git"
    },
    "keywords": [
        "typescript",
        "asynchronous",
        "error-handling",
        "async-await"
    ],
    "main": "dist/Outcome.js",
    "files": [
        "dist/types/src/Outcome.d.ts"
    ],
    "types": "dist/types/src/Outcome.d.ts",
    "devDependencies": {
        "@ethossoftworks/knock-on-wood": "^1.0.2",
        "clean-webpack-plugin": "3.0.0",
        "prettier": "^1.19.1",
        "ts-loader": "^6.2.1",
        "typescript": "3.7.4",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10"
    },
    "scripts": {
        "format": "npx prettier --write --loglevel warn src/**/*.ts test/**/*.ts",
        "build": "yarn format && npx webpack --env prod",
        "test": "yarn format && npx webpack --env test && node dist/Outcome.test.js",
        "test-inspect": "yarn format && npx webpack --env test && node --inspect-brk dist/Outcome.test.js"
    },
    "prettier": {
        "semi": false,
        "printWidth": 120,
        "tabWidth": 4,
        "useTabs": false
    }
}